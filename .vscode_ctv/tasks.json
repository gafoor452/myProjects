{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "doit build",
            "type": "shell",
            "command": "doit build -t run_unittest__all",
            "isBackground": false,
            "problemMatcher": {
                "fileLocation": [
                    "relative",
                    "${workspaceFolder}/bin/cmake-Unittest/"
                ],
                "base": "$gcc",
                "owner": "cpp",
                "pattern": {
                    "regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            },
            "presentation": {
                "reveal": "always"
            },
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
